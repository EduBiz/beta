<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
      
  <field name="campaignname">
        <field-validator type="requiredstring">
            <message>Campaign Name is required.</message>
        </field-validator>  
  </field>
  <field name="dailybdgt">
        <field-validator type="required">
            <message>Budget Value is required.</message>
        </field-validator>  
  </field>
   <field name="enddate">
       <field-validator type="required" >  
            <message>Select Date.</message>
        </field-validator> 
  </field>
             
</validators>

